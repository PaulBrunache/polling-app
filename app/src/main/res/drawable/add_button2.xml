<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:gravity="center" >
        <shape android:shape="oval">
            <solid android:color="@color/selector_ui_button"/>
            <!--<solid android:color="@color/color_clear"/>-->
            <size android:height="@dimen/FAB_diameter"
                android:width="@dimen/FAB_diameter"/>
        </shape>
    </item>

    <item android:left="12dp" android:right="12dp" android:gravity="center">
        <shape android:shape="line">
            <stroke android:color="@color/color_white"
                android:width="3dp"/>
            <solid android:color="@color/color_white"/>
        </shape>
    </item>



    <item android:left="12dp" android:right="12dp"   android:gravity="center">
        <rotate android:fromDegrees="90" android:toDegrees="90">
            <shape android:shape="line">
                <stroke android:color="@color/color_white"
                    android:width="3dp"/>
                <solid android:color="@color/color_white"/>

            </shape>
        </rotate>

    </item>


</layer-list>